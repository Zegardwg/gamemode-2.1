						bData[bizid][bProd]--;
						bData[bizid][bMoney] += price;
						Bisnis_Save(bizid);
						SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DRINK_SPRUNK);
						SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s telah membeli minuman seharga %s.", ReturnName(playerid), FormatMoney(price));
